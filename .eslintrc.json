{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:react/recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint", "react", "react-native", "import"
        
    ],
    "rules": {
        "@typescript-eslint/explicit-function-return-type": "off",
        "import/no-duplicates": ["error", {"prefer-inline": true}]
    },
    "settings": {
    "import/resolver": {
      "babel-module": {
        "alias": {
          "@icons": "./assets/icons"
        }
      }
    }
  }
}
